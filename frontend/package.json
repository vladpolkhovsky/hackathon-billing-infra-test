{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "^20",
    "pnpm": "^8"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --quiet --cache --report-unused-disable-directives src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --fix --quiet --cache --report-unused-disable-directives src/**/*.{ts,tsx,js,jsx}",
    "format": "prettier --write --ignore-unknown .",
    "schema": "openapi-typescript http://localhost/api/billing/v3/api-docs --output src/api.ts"
  },
  "dependencies": {
    "openapi-fetch": "^0.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.14",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.1",
    "openapi-typescript": "^7.9.1",
    "prettier": "^3.2.4",
    "typescript": "^5.9.3",
    "vite": "^5.0.8"
  }
}
